public class Person {
    private String first;
    private String last;
    private int age2;
    private String ssn2;

    public Person(String firstName, String lastName, int age, String ssn) {
        first = firstName;
        last = lastName;
        age2 = age;
        ssn2 = ssn;
    }
    public String toString() {
        return "SSN: " + ssn2 + "\n\t" + "Name: " + first + " " + last + "\n\t" + "Age: " + age2;
    }
}

// Passed