import java.util.Scanner;
import shapes.*;


public class U5_L4_Activity_Three {

    public static double circDiff(Circle c1, Circle c2) {
        double diff = Math.abs(c1.getCircumference() - c2.getCircumference());
        return diff;
    }
}

