/* Lesson 2 Coding Activity Question 2 */

import java.util.Scanner;

public class U3_L2_Activity_Two
{
  public static void main(String[] args)
  {
    Scanner scan = new Scanner(System.in); // scanner
    System.out.println("Please enter two numbers:"); // ask for two numbers
    double a = scan.nextDouble(); // record answers
    double b = scan.nextDouble();
    if (a <= b) // if a is smallest
        System.out.println("Smallest is: " + a); // result
    if (a > b) // if b is smallest
        System.out.println("Smallest is: " + b);    // result 
  }
}

// Passed