/* Lesson 2 Coding Activity Question 3 */

import java.util.Scanner;

public class U3_L2_Activity_Three
{
  public static void main(String[] args)
  {
    Scanner scan = new Scanner(System.in); // scanner
    System.out.println("What is the temperature?"); // ask question
    double x = scan.nextDouble(); // record double
    if ( x < 97) { // if x smaller than 97
        System.out.println("NOT NORMAL"); // print result
    }   else if (x > 99) { // if x larger than 99
        System.out.println("NOT NORMAL"); // print result
    }

  }
}

// Passed