/* Lesson 2 Coding Activity Question 4 */

import java.util.Scanner;

public class U3_L2_Activity_Four
{
  public static void main(String[] args)
  {
    Scanner scan = new Scanner(System.in); // scanner
    System.out.println("Enter two test scores:"); // ask question
    double x = scan.nextDouble(); // scan ans1
    double y = scan.nextDouble(); // scan ans2
    if (x > 100) { // if x larger than 11
        System.out.println("Not Valid"); // print result
    }   else if (x < 0) { // if x smaller than 0
        System.out.println("Not Valid"); // print result
    }
    if (y > 100) { // if y larger than 100
        System.out.println("Not Valid"); // print result
    }   else if (y < 0) { // if y smaller than 0
        System.out.println("Not Valid"); // print result
    }
  }
}

// Passed