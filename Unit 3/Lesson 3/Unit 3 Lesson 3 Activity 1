/* Lesson 3 Coding Activity Question 1 */

import java.util.Scanner;

public class U3_L3_Activity_One
{
  public static void main(String[] args)
  {
    Scanner scan = new Scanner(System.in); // scanner
    System.out.println("Please enter an integer"); // ask question
    int x = scan.nextInt(); // record integer
    if (x % 2 == 0) { // if x is divisble by 2 or equal to 0
        System.out.println("Even"); // print result
    } else {
        System.out.println("Odd"); // else is odd
    }
  }
}

// Passed