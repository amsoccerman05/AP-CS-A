    /* Lesson 1 Coding Activity Question 1 */

    import java.util.Scanner;

    public class U3_L1_Activity_One
    {
    public static void main(String[] args)
    {
        Scanner scan = new Scanner(System.in); // scanner
        System.out.println("Please enter a double:"); // ask for double
        double x = scan.nextDouble(); // record double
        if (x == 12.345) // test if double = 12.345
            System.out.println("YES");  // result if yes
    }
    }

// Passed