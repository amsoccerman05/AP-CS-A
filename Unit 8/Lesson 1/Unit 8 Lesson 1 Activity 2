public class U8_L1_Activity_Two {

    public static int[][] productTable(int columns, int rows) {
        int arr[][] = new int[columns][rows];

        for (int i = 0; i < columns; i++) {
            for (int k = 0; k < rows; k++) {
                arr[i][k] = i * k;
            }
        }
        return arr;
    }

}

// Passed